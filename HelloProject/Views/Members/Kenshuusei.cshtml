@{
    ViewData["Title"] = "Home Page";
}

<!DOCTYPE html>

<html>

<head>
    <link rel="shortcut icon" href="~/assets/icons/Hermandad.ico" type="image/x-icon">
    <link rel="icon" href="~/assets/icons/Hermandad.ico" type="image/x-icon">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta charset="utf-8">
    <meta name="og:site_name" content="Hello! Project Sorter">
    <meta name="og:description" content="A simple website for sorting H!P idols in a formatted list.">
    <meta name="og:image" content="https://i.imgur.com/IZzJMk6.jpg">
    <title>Hello! Projec Sorting</title>

    @*<link rel="stylesheet" type="text/css" href="~/css/reset.css">
    <link rel="stylesheet" type="text/css" href="~/css/styles.css">*@

    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,800" rel="stylesheet">
    <link crossorigin="anonymous" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css" integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS" rel="stylesheet">
    <link href="~/css/styles4.css" rel="stylesheet" type="text/css">

    <script src="~/js/data.js"></script>
    <script src="~/js/data/2021-03-29.js"></script>

    <script src="~/js/dom-to-image.min.js"></script>
    <script src="~/js/html2canvas.min.js"></script>
    <script src="~/js/lz-string.min.js"></script>
    <script src="~/js/seedrandom.min.js"></script>
    <script src="~/js/main.js"></script>

    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-64784129-2"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-64784129-2');
    </script>

</head>

<body>
    <div class="container">

        <div class="titlehp row" style="text-align: center;align-items: center;align-content: center;">
            <div class="col-12 box" style="text-align: center;align-items: center;align-content: center;">
                <p><b>Hello! Project Member List</b></p>
            </div>
        </div>

        <div class="row" style="justify-content:center; margin-bottom:-20px;">
            <a class="col-2 box" style="align-content:center; margin:10px 50px 50px 10px" asp-controller="Members" asp-action="MorningMusume">
                <div class="option-item">
                    <input id="momusu" type="radio" name="group" class="option-checkbox">
                    <div class="image-wrapper">
                        <img src="~/assets/icons/MNMSM2400.jpg" alt="Morning Musume '24" class="option-image">
                        <div class="option-text">Morning Musume '24</div>
                    </div>
                </div>
            </a>
            <a class="col-2 box" style="align-content:center; margin:10px 50px 50px 10px" asp-controller="Members" asp-action="Angerme">
                <div class="option-item">
                    <input id="angerme" type="radio" name="group" class="option-checkbox">
                    <div class="image-wrapper">
                        <img src="~/assets/icons/ANGM00.jpg" alt="ANGERME" class="option-image">
                        <div class="option-text">ANGERME</div>
                    </div>
                </div>
            </a>
            <a class="col-2 box" style="align-content:center; margin:10px 50px 50px 10px" asp-controller="Members" asp-action="JuiceJuice">
                <div class="option-item">
                    <input id="juice" type="radio" name="group" class="option-checkbox">
                    <div class="image-wrapper">
                        <img src="~/assets/icons/JCJC00.jpg" alt="Juice=Juice" class="option-image">
                        <div class="option-text">Juice=Juice</div>
                    </div>
                </div>
            </a>
            <a class="col-2 box" style="align-content:center; margin:10px 50px 50px 10px" asp-controller="Members" asp-action="TsubakiFactory">
                <div class="option-item">
                    <input id="tsubaki" type="radio" name="group" class="option-checkbox">
                    <div class="image-wrapper">
                        <img src="~/assets/icons/TBKFTR00.jpg" alt="Tsubaki Factory" class="option-image">
                        <div class="option-text">Tsubaki Factory</div>
                    </div>
                </div>
            </a>
        </div>

        <div class="row" style="justify-content:center; margin-bottom:-20px;">
            <a class="col-2 box" style="align-content:center; margin:10px 50px 50px 10px" asp-controller="Members" asp-action="Beyonds">
                <div class="option-item">
                    <input id="beyond" type="radio" name="group" class="option-checkbox">
                    <div class="image-wrapper">
                        <img src="~/assets/icons/BYNDS00.jpg" alt="BEYOOOOONDS" class="option-image">
                        <div class="option-text">BEYOOOOONDS</div>
                    </div>
                </div>
            </a>
            <a class="col-2 box" style="align-content:center; margin:10px 50px 50px 10px" asp-controller="Members" asp-action="OchaNorma">
                <div class="option-item">
                    <input id="ocha" type="radio" name="group" class="option-checkbox">
                    <div class="image-wrapper">
                        <img src="~/assets/icons/OCNM00.jpg" alt="OCHA NORMA" class="option-image">
                        <div class="option-text">OCHA NORMA</div>
                    </div>
                </div>
            </a>
            <a class="col-2 box" style="align-content:center; margin:10px 50px 50px 10px" asp-controller="Members" asp-action="RosyChronicle">
                <div class="option-item">
                    <input id="rosy" type="radio" name="group" class="option-checkbox">
                    <div class="image-wrapper">
                        <img src="~/assets/icons/RSCNC00.jpg" alt="Rosy Chronicle" class="option-image">
                        <div class="option-text">Rosy Chronicle</div>
                    </div>
                </div>
            </a>
            <a class="col-2 box" style="align-content:center; margin:10px 50px 50px 10px" asp-controller="Members" asp-action="Kenshuusei">
                <div class="option-item">
                    <input id="kss" type="radio" name="group" class="option-checkbox">
                    <div class="image-wrapper">
                        <img src="~/assets/idols/KSS00.jpg" alt="Hello Pro Kenshuusei" class="option-image">
                        <div class="option-text">Hello Pro Kenshuusei</div>
                    </div>
                </div>
            </a>
        </div>

        <div class="row" style="justify-content:center; margin-bottom:-20px;">
            <a class="col-2 box" style="align-content:center; margin:10px 50px 50px 10px" asp-controller="Members" asp-action="Cute">
                <div class="option-item">
                    <input id="cute" type="radio" name="group" class="option-checkbox">
                    <div class="image-wrapper">
                        <img src="~/assets/idols/CUTE00.jpg" alt="°C-ute" class="option-image">
                        <div class="option-text">°C-ute</div>
                    </div>
                </div>
            </a>
            <a class="col-2 box" style="align-content:center; margin:10px 50px 50px 10px" asp-controller="Members" asp-action="BerryzKoubou">
                <div class="option-item">
                    <input id="berryz" type="radio" name="group" class="option-checkbox">
                    <div class="image-wrapper">
                        <img src="~/assets/idols/BRKB00.jpg" alt="Berryz Koubou" class="option-image">
                        <div class="option-text">Berryz Koubou</div>
                    </div>
                </div>
            </a>
            <a class="col-2 box" style="align-content:center; margin:10px 50px 50px 10px" asp-controller="Members" asp-action="KobushiFactory">
                <div class="option-item">
                    <input id="kobushi" type="radio" name="group" class="option-checkbox">
                    <div class="image-wrapper">
                        <img src="~/assets/idols/KBSFTR00.jpg" alt="Kobushi Factory" class="option-image">
                        <div class="option-text">Kobushi Factory</div>
                    </div>
                </div>
            </a>
            <a class="col-2 box" style="align-content:center; margin:10px 50px 50px 10px" asp-controller="Members" asp-action="CountryGirls">
                <div class="option-item">
                    <input id="country" type="radio" name="group" class="option-checkbox">
                    <div class="image-wrapper">
                        <img src="~/assets/idols/CTGRLS00.jpg" alt="Country Girls" class="option-image">
                        <div class="option-text">Country Girls</div>
                    </div>
                </div>
            </a>
        </div>

    </div>

    <div class="container">
        <div class="row">
            <div class="col-12 info">
                <p>By <a href="https://x.com/rodneyamarilla1">Nakajima</a> Modified from <a href="https://github.com/execfera/charasort/">charasort by execfera</a><br>Data updated by <a href="https://x.com/rodneyamarilla1">Nakajima</a></p>
                <p>This site is just a fan site and doesn't affiliate with UP-FRONT PROMOTION.</p>
                <p>Dedicado a la Hermandad H!P.</p>
            </div>
        </div>
    </div>
</body>

</html>
